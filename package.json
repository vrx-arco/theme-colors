{
  "name": "@vrx-arco/theme-colors",
  "version": "0.5.2",
  "packageManager": "pnpm@9.9.0",
  "description": "ArcoDesign color utils",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup-node",
    "test": "vitest",
    "release": "vbumpp -r && pnpm build",
    "update:deps": "taze major -r -w -i -f"
  },
  "keywords": [
    "ArcoDesign",
    "Color",
    "utils"
  ],
  "repository": "https://github.com/vrx-arco/theme-colors.git",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ctrl/tinycolor": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "@vill-v/bumpp-github": "^2.1.2",
    "@vill-v/eslint-config": "^1.5.2",
    "@vill-v/prettier-config": "^1.0.0",
    "@vill-v/tsconfig": "^1.0.1",
    "eslint": "^9.9.1",
    "prettier": "^3.3.3",
    "taze": "^0.16.7",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
